<?xml version="1.0" encoding="UTF-8"?>
<teamcity-plugin xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:schemas-jetbrains-com:teamcity-plugin-v1-xml">
  <info>
    <name>autopin</name>
    <display-name>autopin</display-name>
    <version>@Version@</version>
    <description>This Teamcity plugin allows to pin and/or tag builds automatically</description>
    <download-url>https://github.com/aesirinteractive/teamcity-autopin</download-url>
    <email>aesirdevops@aesir-interactive.com</email>
    <vendor>
      <name>@VendorName@</name>
      <url>@VendorURL@</url>
      <logo>nologo</logo>
    </vendor>
  </info>
  <deployment use-separate-classloader="true" />
</teamcity-plugin>
